{{- $scratch := newScratch -}}
{{- $scratch.Add "export" slice -}}
{{- range $date, $times := .Site.Data.leaderboards -}}
	{{- $scratch.Add "current" slice -}}
	{{- range $times -}}
		{{-
			$scratch.Add "current" (dict
				"name" .name
				"seconds" .fullTimeInSeconds
			)
		-}}
	{{- end -}}
	{{-
		$scratch.Add "export" (dict
			"date" $date
			"times" ($scratch.Get "current")
		)
	-}}
	{{- $scratch.Delete "current" -}}
{{- end -}}
{{ jsonify ($scratch.Get "export") }}
